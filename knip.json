{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/treemap-card.ts", "src/index.ts"],
  "project": ["src/**/*.ts"],
  "ignore": ["src/**/*.test.ts"],
  "ignoreDependencies": [],
  "ignoreBinaries": ["heroshot", "perl"],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "devDependencies": "error",
    "optionalPeerDependencies": "error",
    "unlisted": "error",
    "binaries": "error",
    "unresolved": "error",
    "exports": "error",
    "nsExports": "error",
    "classMembers": "error",
    "types": "error",
    "nsTypes": "error",
    "enumMembers": "error",
    "duplicates": "error"
  }
}
